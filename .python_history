from selenium import webdriver
from selenium.webdriver.chrome.options import Options
from bs4 import BeautifulSoup
import time
chrome_options = Options()
chrome_options.add_argument("--headless")
chrome_options.add_argument("--no-sandbox")
chrome_options.add_argument("--disable-dev-shm-usage")
driver = webdriver.Chrome(options=chrome_options)
driver.get("https://www.finn.no/bap/forsale/search.html?q=flislim")
time.sleep(5)
html = driver.page_source
driver.quit()
soup = BeautifulSoup(html, 'html.parser')
listings = soup.find_all("article", class_="ads__unit")
print(f"Found {len(listings)} listings.")
for listing in listings[:5]:
    title_tag = listing.find("a", class_="ads__unit__link")
    if title_tag:
        print(title_tag.get_text(strip=True))
from selenium import webdriver
from selenium.webdriver.chrome.options import Options
from bs4 import BeautifulSoup
import time
chrome_options = Options()
chrome_options.add_argument("--headless=new")
chrome_options.add_argument("--no-sandbox")
chrome_options.add_argument("--disable-dev-shm-usage")
driver = webdriver.Chrome(options=chrome_options)
driver.get("https://www.finn.no/bap/forsale/search.html?q=flislim")
time.sleep(5)
html = driver.page_source
driver.quit()
soup = BeautifulSoup(html, 'html.parser')
listings = soup.find_all("article")
print(f"APTIKTA {len(listings)} <article> elementų")
for l in listings[:5]:
    print("---")
    print(l.prettify()[:500])  # Parodys pirmus 500 simbolių iš įrašo
from selenium import webdriver
from selenium.webdriver.chrome.options import Options
import time
chrome_options = Options()
chrome_options.add_argument("--headless=new")
chrome_options.add_argument("--no-sandbox")
chrome_options.add_argument("--disable-dev-shm-usage")
driver = webdriver.Chrome(options=chrome_options)
driver.get("https://www.byggmakker.no/sok/?q=flislim")
time.sleep(5)
# Išsaugom HTML ir Screenshot į /tmp/
with open("/tmp/byggmakker_debug.html", "w", encoding="utf-8") as f:
    f.write(driver.page_source)
driver.save_screenshot("/tmp/byggmakker_debug.png")
driver.quit()
